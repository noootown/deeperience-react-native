{
  "name": "deeperience",
  "version": "0.0.1",
  "private": true,
  "description": "deeperience app",
  "jest": {
    "preset": "jest-react-native",
    "moduleNameMapper": {
      "\\./datepicker$": "<rootDir>/node_modules/tcomb-form-native/lib/templates/bootstrap/datepicker.ios.js"
    },
    "preprocessorIgnorePatterns": [
      "node_modules/(?!react-native|tcomb-form-native)"
    ],
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/src/reducers/auth/(.*SagaTest.js)",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "ios.js",
      "android.js"
    ]
  },
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/deeperience/deeperience-app.git"
  },
  "scripts": {
    "test": "jest && npm run mocha",
    "jw": "./node_modules/.bin/jest --watch -o",
    "mocha": "node_modules/.bin/mocha --require testHelper.js --compilers js:babel-core/register src/reducers/*/__tests__/*SagaTest.js",
    "flow": "flow",
    "precommit": "npm run lint && flow && npm run test",
    "start": "react-native start",
    "lint": "./node_modules/.bin/eslint --config=./.eslintrc.json ./src",
    "fix-lint": "/usr/local/bin/eslint --fix ./src",
    "test:watch": "npm run test -- --watch",
    "test-chrome": "node-debug --preload false --nodejs --harmony jest --runInBand",
    "docs": "./node_modules/docker/docker -w -i src/ -o ../snowflake-pages "
  },
  "pre-commit": [
    "precommit"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "eslint": "^3.7.1",
    "firebase": "^3.3.0",
    "immutable": "3.8.1",
    "invariant": "^2.2.1",
    "jest-cli": "^15.1.1",
    "key-mirror": "1.0.1",
    "react": "15.3.2",
    "react-mixin": "^2.0.2",
    "react-native": "0.34",
    "react-native-animatable": "^0.6.1",
    "react-native-app-intro": "^1.1.5",
    "react-native-button": "^1.7.0",
    "react-native-facebook-login": "^1.3.0",
    "react-native-i18n": "0.1.1",
    "react-native-mock": "^0.2.7",
    "react-native-modal-picker": "0.0.16",
    "react-native-multi-slider": "^0.3.5",
    "react-native-router-flux": "3.35.0",
    "react-native-scrollable-tab-view": "^0.6.0",
    "react-native-simple-store": "1.0.1",
    "react-native-simpledialog-android": "1.0.7",
    "react-native-slider": "^0.9.1",
    "react-native-vector-icons": "2.0.3",
    "react-redux": "4.4.5",
    "react-test-renderer": "^15.3.2",
    "react-timer-mixin": "^0.13.3",
    "redux": "3.5.2",
    "redux-logger": "^2.7.0",
    "redux-saga": "^0.11.1",
    "redux-storage": "^4.1.1",
    "redux-storage-decorator-debounce": "^1.1.1",
    "redux-storage-engine-reactnativeasyncstorage": "^1.0.2",
    "redux-thunk": "2.1.0",
    "regenerator": "0.8.46",
    "tcomb-form-native": "0.5.3",
    "transit-immutable-js": "^0.6.0",
    "underscore": "1.8.3",
    "validate.js": "0.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "14.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react-native": "^1.8.0",
    "babel-register": "^6.14.0",
    "babel-tape-runner": "^2.0.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chai-immutable": "^1.6.0",
    "concurrently": "^2.2.0",
    "dirty-chai": "^1.2.2",
    "docker": "1.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "flow-bin": "^0.32.0",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-if": "^2.0.1",
    "gulp-react-native-stylesheet-css": "^1.4.4",
    "gulp-sass": "^2.3.2",
    "gulp-watch": "^4.3.9",
    "immutable-devtools": "0.0.7",
    "istanbul": "0.4.3",
    "jest": "^15.1.1",
    "jest-react-native": "^16.0.0",
    "mocha": "^3.0.2",
    "mockery": "^1.7.0",
    "path": "^0.12.7",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "15.0.2",
    "react-dom": "^15.3.1",
    "reactotron-react-native": "^1.1.2",
    "redux-mock-store": "1.1.2",
    "remote-redux-devtools": "^0.4.8",
    "rnpm-plugin-upgrade": "0.26.0",
    "sinon": "^1.17.5"
  }
}
